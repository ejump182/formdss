
<{$formulize.javascript}>
<form name="<{$formulize.name}>" action="<{$formulize.action}>" method="<{$formulize.method}>" <{$formulize.extra}>>
<{if $formallowsviews}>
<{if $theycanadd}>
<center>
	<table>
	<tr>
	<td width=25%>&nbsp;</td>
	<td>
		<table cellpadding=3 cellspacing=1 border=1>
		<tr>
		<th valign=middle>
			<center><h3><nobr><b><{$tempaddingentry}></b></nobr></h3></center>
		</th>
		<td class=head valign=middle>
			<center><h3><nobr><a href="<{$tempformurl}>&select=1"><{$tempviewentries}></a></nobr></h3></center>
		</td>
		</tr>
		</table>
	</td>
	<td width=25%>&nbsp;</td>
	</tr>
	</table>
</center>
	<hr>
<{/if}>
<{/if}>
	<table cellspacing="1" class="outer"  width=98%>
		<tr><th colspan="2"><{$formulize.title}></th></tr>
		<{foreach item=element from=$formulize.elements}>
		<{if $element.hidden != true}>
		<tr>
			<td class="head"><{$element.caption}></td>
			<td class="<{cycle values="even,odd"}>"><{$element.body}></td>
		</tr>
		<{/if}>
		<{/foreach}>
	</table>
	<{foreach item=element from=$formulize.elements}>
	<{if $element.hidden == true}>
		<{$element.body}>
	<{/if}>
	<{/foreach}>
</form>
