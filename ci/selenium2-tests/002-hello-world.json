{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "store",
      "text": "/home/travis/build/jegelstaff/formulize",
      "variable": "web_root"
    },
    {
      "type": "store",
      "text": "localhost",
      "variable": "test_domain"
    },
    {
      "type": "store",
      "text": "root",
      "variable": "db_user"
    },
    {
      "type": "store",
      "text": "password",
      "variable": "db_pass"
    },
    {
      "type": "store",
      "text": "formulize",
      "variable": "db_name"
    },
    {
      "type": "store",
      "text": "password",
      "variable": "admin_password"
    },
    {
      "type": "store",
      "text": "short_wait",
      "variable": "234"
    },
    {
      "type": "store",
      "text": "save_wait",
      "variable": "1525"
    },
    {
      "type": "get",
      "url": "http://${test_domain}/modules/formulize/application.php?id=all"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "uname"
      },
      "text": "admin"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "pass"
      },
      "text": "${admin_password}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
      "type": "pause",
      "waitTime": "543"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Admin"
      }
    },
    {
      "type": "pause",
      "waitTime": "543"
    },
    {
      "type": "verifyTextPresent",
      "text": ""
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}