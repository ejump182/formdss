{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost/index.php"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "uname"
      },
      "text": "admin"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "pass"
      },
      "text": "password"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "p > a"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "firstbox"
      },
      "text": "blue AND science"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Client Name"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Blue Sea Ocean Science"
    },
    {
      "type": "verifyTextPresent",
      "negated": true,
      "text": "Humans For Humanity"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#innerAddEntryPanel > p"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "formulize_resetViewButton"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "search_client_city"
      },
      "text": "vancouver OR toronto"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "City"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Vancouver"
    },
    {
      "type": "verifyTextPresent",
      "text": "Toronto"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}